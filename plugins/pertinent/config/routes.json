{
  "routes": [
    {
      "method": "GET",
      "path": "/pages/meta",
      "handler": "Page.meta",
      "config": {
        "policies": ["plugins::users-permissions.rateLimit"]
      }
    },
    {
      "method": "POST",
      "path": "/pages/slug",
      "handler": "Page.slug",
      "config": {
        "policies": ["plugins::users-permissions.rateLimit"]
      }
    },
    {
      "method": "POST",
      "path": "/pages",
      "handler": "Page.create",
      "config": {
        "policies": ["plugins::users-permissions.rateLimit"]
      }
    },
    {
      "method": "GET",
      "path": "/pages/:id",
      "handler": "Page.getById",
      "config": {
        "policies": ["plugins::users-permissions.rateLimit"]
      }
    },
    {
      "method": "PUT",
      "path": "/pages/:id",
      "handler": "Page.update",
      "config": {
        "policies": ["plugins::users-permissions.rateLimit"]
      }
    },
    {
      "method": "DELETE",
      "path": "/pages/:id",
      "handler": "Page.delete",
      "config": {
        "policies": ["plugins::users-permissions.rateLimit"]
      }
    },
    {
      "method": "POST",
      "path": "/tags",
      "handler": "Tag.create",
      "config": {
        "policies": ["plugins::users-permissions.rateLimit"]
      }
    },
    {
      "method": "PUT",
      "path": "/tags/:id",
      "handler": "Tag.update",
      "config": {
        "policies": ["plugins::users-permissions.rateLimit"]
      }
    },
    {
      "method": "DELETE",
      "path": "/tags/:id",
      "handler": "Tag.delete",
      "config": {
        "policies": ["plugins::users-permissions.rateLimit"]
      }
    }
  ]
}
